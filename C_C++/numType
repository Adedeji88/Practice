#include <stdio.h>

/**
* numType - function to check whether a number is even or odd
* Return: 0.
*/

int numType(int a)
{
	if((a % 2) == 0)
		return(1);
	else
		return(0);
}

int main(void)
{
	int res, n;
	printf("Enter a number: ");
	scanf("%d", &n);
	
	res = numType(n);
	
	if(res == 1)
		printf("%d is even\n", n);
	else
		printf("%d is odd\n", n);
		
	return(0);
}