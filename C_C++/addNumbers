#include <stdio.h>

/**
* addNumbers - function to add 2 numbers
* 
* Return: 0.
*/

int addNumbers(int a, int b)
{
	int result;
	result = a + b;
	return(result);
}

int main(void)
{
	int n1, n2, sum;
	printf("Enter two integers: ");
	scanf("%d %d", &n1, &n2);
	
	sum = addNumbers(n1, n2);
	printf("sum = %d", sum);
	
	return(0);
}