#include <stdio.h>

/**
* mulNumbers - function to multiply 2 numbers
* 
* Return: 0.
*/

int mulNumbers(int a, int b)
{
	int product;
	product = a * b;
	return(product);
}

int main(void)
{
	int n1, n2, prod;
	printf("Enter two integers: ");
	scanf("%d %d", &n1, &n2);
	
	prod = mulNumbers(n1, n2);
	printf("Product = %d", prod);
	
	return(0);
}